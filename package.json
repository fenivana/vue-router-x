{
  "name": "vue-stateful-router",
  "version": "0.6.1",
  "description": "A vue router with the power of history.state",
  "main": "dist/vueStatefulRouter.umd.js",
  "module": "dist/vueStatefulRouter.esm.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/vue-stateful-router.git"
  },
  "keywords": [
    "vue",
    "router"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "dependencies": {
    "spa-history": "^0.6.1",
    "url-router": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-enough": "^0.3.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "bugs": {
    "url": "https://github.com/fenivana/vue-stateful-router/issues"
  },
  "homepage": "https://github.com/fenivana/vue-stateful-router#readme"
