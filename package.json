{
  "name": "vue-router-x",
  "version": "0.1.0-dev.0",
  "description": "If the official router can't meet your needs",
  "main": "dist/Router.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "babel src -w -d dist",
    "build": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/vue-router-x.git"
  },
  "keywords": [
    "vue",
    "router"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "1.3.3",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-enough": "^0.2.6"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": "umd"
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "bugs": {
    "url": "https://github.com/fenivana/vue-router-x/issues"
  },
  "homepage": "https://github.com/fenivana/vue-router-x#readme"
}
