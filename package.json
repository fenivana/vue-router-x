{
  "name": "vue-stateful-router",
  "version": "0.9.0",
  "description": "A vue router with the power of history.state",
  "main": "dist/vueStatefulRouter",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "rollup -c rollup.config.esm.mjs && rollup -c rollup.config.umd.mjs && rollup -c rollup.config.umd.bundle.mjs",
    "example": "ws --spa example.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangfengming/vue-stateful-router.git"
  },
  "keywords": [
    "vue",
    "router"
  ],
  "author": "Jiang Fengming <jiangfengming87@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jiangfengming/vue-stateful-router#readme",
  "bugs": {
    "url": "https://github.com/jiangfengming/vue-stateful-router/issues"
  },
  "dependencies": {
    "spa-history": "^0.7.0",
    "url-router": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-config-enough": "^0.3.4",
    "local-web-server": "^2.6.0",
    "rollup": "^0.68.1",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
